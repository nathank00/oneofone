create table public.games (
  "GAME_ID" bigint generated by default as identity not null,
  "CREATED_AT" timestamp with time zone not null default now(),
  "SEASON_ID" bigint null,
  "AWAY_NAME" text null,
  "HOME_NAME" text null,
  "AWAY_ID" bigint null,
  "HOME_ID" bigint null,
  "GAME_STATUS" integer null,
  "GAME_OUTCOME" integer null,
  "GAME_DATE" timestamp with time zone null,
  "AWAY_PTS" smallint null,
  "HOME_PTS" smallint null,
  "TOTAL_PTS" smallint null,
  "HOME_TEAM_PLAYERS" bigint[] null,
  "AWAY_TEAM_PLAYERS" bigint[] null,
  constraint games_pkey primary key ("GAME_ID")
) TABLESPACE pg_default;
