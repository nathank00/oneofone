create table public.gamelogs (
  "GAME_ID" bigint not null,
  "CREATED_AT" timestamp with time zone not null default now(),
  "SEASON_ID" integer null,
  "GAME_DATE" timestamp with time zone null,
  "AWAY_NAME" text null,
  "HOME_NAME" text null,
  "AWAY_ID" bigint null,
  "HOME_ID" bigint null,
  "GAME_STATUS" integer null,
  "GAME_OUTCOME" integer null,
  "AWAY_PTS" smallint null,
  "HOME_PTS" smallint null,
  "TOTAL_PTS" smallint null,
  "AWAY_PTS_10" real null,
  "AWAY_REB_10" real null,
  "AWAY_AST_10" real null,
  "AWAY_STL_10" real null,
  "AWAY_BLK_10" real null,
  "AWAY_TOV_10" real null,
  "AWAY_PF_10" real null,
  "AWAY_FG_PCT_10" real null,
  "AWAY_FG3_PCT_10" real null,
  "AWAY_FT_PCT_10" real null,
  "AWAY_PLUS_MINUS_10" real null,
  "AWAY_WIN_RATE_10" real null,
  "AWAY_GAMES_10" smallint null,
  "HOME_PTS_10" real null,
  "HOME_REB_10" real null,
  "HOME_AST_10" real null,
  "HOME_STL_10" real null,
  "HOME_BLK_10" real null,
  "HOME_TOV_10" real null,
  "HOME_PF_10" real null,
  "HOME_FG_PCT_10" real null,
  "HOME_FG3_PCT_10" real null,
  "HOME_FT_PCT_10" real null,
  "HOME_PLUS_MINUS_10" real null,
  "HOME_WIN_RATE_10" real null,
  "HOME_GAMES_10" smallint null,
  "AWAY_PTS_30" real null,
  "AWAY_REB_30" real null,
  "AWAY_AST_30" real null,
  "AWAY_STL_30" real null,
  "AWAY_BLK_30" real null,
  "AWAY_TOV_30" real null,
  "AWAY_PF_30" real null,
  "AWAY_FG_PCT_30" real null,
  "AWAY_FG3_PCT_30" real null,
  "AWAY_FT_PCT_30" real null,
  "AWAY_PLUS_MINUS_30" real null,
  "AWAY_WIN_RATE_30" real null,
  "AWAY_GAMES_30" smallint null,
  "HOME_PTS_30" real null,
  "HOME_REB_30" real null,
  "HOME_AST_30" real null,
  "HOME_STL_30" real null,
  "HOME_BLK_30" real null,
  "HOME_TOV_30" real null,
  "HOME_PF_30" real null,
  "HOME_FG_PCT_30" real null,
  "HOME_FG3_PCT_30" real null,
  "HOME_FT_PCT_30" real null,
  "HOME_PLUS_MINUS_30" real null,
  "HOME_WIN_RATE_30" real null,
  "HOME_GAMES_30" smallint null,
  "PREDICTION" integer null,
  "PREDICTION_PCT" real null,
  constraint gamelogs_pkey primary key ("GAME_ID")
) TABLESPACE pg_default;

create index IF not exists idx_gamelogs_game_date on public.gamelogs using btree ("GAME_DATE") TABLESPACE pg_default;

create index IF not exists idx_gamelogs_season_id on public.gamelogs using btree ("SEASON_ID") TABLESPACE pg_default;
