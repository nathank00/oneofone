create table public.playerstats (
  id text not null,
  created_at timestamp with time zone not null default now(),
  "GAME_ID" bigint null,
  "PLAYER_ID" bigint null,
  "GAME_DATE" timestamp with time zone null,
  "MATCHUP" text null,
  "WL" character varying null,
  "MIN" smallint null,
  "FGM" smallint null,
  "FGA" smallint null,
  "FG_PCT" real null,
  "FG3M" smallint null,
  "FG3A" smallint null,
  "FG3_PCT" real null,
  "FTM" smallint null,
  "FTA" smallint null,
  "FT_PCT" real null,
  "OREB" smallint null,
  "DREB" smallint null,
  "REB" smallint null,
  "AST" smallint null,
  "STL" smallint null,
  "BLK" smallint null,
  "TOV" smallint null,
  "PF" smallint null,
  "PTS" smallint null,
  "PLUS_MINUS" smallint null,
  "SEASON_ID" integer null,
  constraint playerstats_pkey primary key (id)
) TABLESPACE pg_default;
